    "import re\n",
    "\n",
    "def clean_amharic(text):\n",
    "    # Remove emojis, links, special chars\n",
    "    text = re.sub(r'[^\\w\\s\\u1200-\\u137F]', '', text)\n",
    "    text = re.sub(r'http\\S+|www\\S+|https\\S+', '', text)\n",
    "    return text.strip()\n",
    "\n",
    "df['cleaned_text'] = df['text'].apply(clean_amharic)"
